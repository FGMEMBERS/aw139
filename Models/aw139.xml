<animation>
            <type>rotate</type>
        <object-name>wheelf</object-name>
	     <object-name>Cylinder</object-name>
	        <object-name>upperstrutf</object-name>
		  <object-name>lowerstrutf</object-name>
           <property>/gear/gear[0]/caster-angle-deg</property>
             <center>
                    <x-m>-4.456</x-m>
                    <y-m>0</y-m>
                    <z-m>-1.931</z-m>
		</center>
            <axis>
              <x>0</x>
