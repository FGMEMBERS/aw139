            </axis>
        </animation>

<animation>
            <type>translate</type>
            <object-name>wheelf</object-name>
	     <object-name>Cylinder</object-name>
         <property>/gear/gear[0]/position-norm</property>
            <interpolation>
                    <entry>
                    <ind>1.0</ind>
                    <dep>-0.20</dep>
                    </entry>
		    <entry>
                    <ind>0.5</ind>
                    <dep>-0.13</dep>
                    </entry>
		<entry>
                    <ind>0</ind>
                    <dep>0.0</dep>
                    </entry>
	</interpolation>
	
            <axis>
              <x>0</x>
              <y>0</y>
              <z>1</z>
            </axis>
        </animation>


<animation>
            <type>translate</type>
            <object-name>wheelf</object-name>
	     <object-name>Cylinder</object-name>
            <property>/gear/gear[0]/compression-norm</property>
            <interpolation>
                    <entry>
                    <ind>0.863</ind>
                    <dep>0.5211</dep>
                    </entry>
		<entry>
                    <ind>0.332</ind>
                    <dep>0.323</dep>
                    </entry>
		<entry>
		<ind>0</ind>
                    <dep>0</dep>
                    </entry>
	</interpolation>
	
            <axis>
              <x>0</x>
              <y>0</y>
              <z>1</z>
            </axis>
        </animation>
	
<!--variant select animation-->

