<animation>
            <type>rotate</type>
        <object-name>wheelf</object-name>
	     <object-name>Cylinder</object-name>
	        <object-name>upperstrutf</object-name>
		  <object-name>lowerstrutf</object-name>
           <property>/gear/gear[0]/position-norm</property>
            <interpolation>
                   <entry>
                    <ind>1.0</ind>
                    <dep>0</dep>
                    </entry>
		  <entry>
                    <ind>0</ind>
                    <dep>110</dep>
                    </entry>
	</interpolation>
	 <center>
                    <x-m>-4.456</x-m>
