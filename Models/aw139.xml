<?xml version="1.0"?>

<PropertyList>
 	<path>AW139.ac</path>
	<offsets>
		<z-m>0</z-m>
		<y-m>0.0</y-m>
		<x-m>0</x-m>

		<pitch-deg>0</pitch-deg>
	</offsets>
	
  <nasal>
        <load>
            var livery_update = aircraft.livery_update.new(
	    "Aircraft/aw139/Models/liveries", 10,
	    func { print("aw139 livery update") });
        </load>

        <unload>
            livery_update.stop();
        </unload>
    </nasal>
  
<animation>
            <type>rotate</type>
            <object-name>wheelr</object-name>
	     <object-name>lowerstrutr</object-name>
	         <object-name>upperstrutr</object-name>
           <property>/gear/gear[2]/position-norm</property>
            <interpolation>
                   <entry>
                    <ind>1.0</ind>
                    <dep>0</dep>
                    </entry>
		  <entry>
                    <ind>0</ind>
                    <dep>90</dep>
                    </entry>
	</interpolation>
	 <center>
                    <x-m>0.022</x-m>
                    <y-m>1.439</y-m>
                    <z-m>-1.931</z-m>
		</center>
            <axis>
              <x>-1</x>
              <y>0</y>
              <z>0</z>
            </axis>
        </animation>
<animation>
            <type>rotate</type>
            <object-name>wheelr</object-name>
	     <object-name>lowerstrutr</object-name>
            <property>/gear/gear[2]/position-norm</property>
            <interpolation>
	          <entry>
                    <ind>0.0</ind>
                    <dep>60</dep>
                    </entry>
                    <entry>
                    <ind>0.7</ind>
                    <dep>45</dep>
                    </entry>
		<entry>
                    <ind>1</ind>
                    <dep>-18.75</dep>
                    </entry>
	</interpolation>
	 <center>
                    <x-m>-0.602</x-m>
                    <y-m>0</y-m>
                    <z-m>-2.131</z-m>
		</center>
            <axis>
              <x>0</x>
              <y>-1</y>
              <z>0</z>
            </axis>
        </animation>

<animation>
            <type>rotate</type>
