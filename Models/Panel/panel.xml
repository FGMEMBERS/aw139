        </animation>
<animation>
            <type>rotate</type>
            <object-name>wheelL</object-name>
	     <object-name>lowerstrutl</object-name>
            <property>/gear/gear[1]/compression-norm</property>
            <interpolation>
                    <entry>
                    <ind>0.0</ind>
                    <dep>0</dep>
                    </entry>
		 <entry>
                    <ind>0.4</ind>
                    <dep>-15</dep>
                    </entry>
		 <entry>
                    <ind>0.525</ind>
                    <dep>-22</dep>
                    </entry>
		   <entry>
                    <ind>1</ind>
                    <dep>-28</dep>
                    </entry>
	</interpolation>
	 <center>
                    <x-m>1.092</x-m>
                    <y-m>-1.360</y-m>
                    <z-m>-1.391</z-m>
		</center>
            <axis>
              <x>0</x>
              <y>1</y>
              <z>0</z>
            </axis>
        </animation>

<animation>
            <type>rotate</type>
        <object-name>wheelFR</object-name>
	     <object-name>Cylinder</object-name>
	        <object-name>frntgear-upcyl</object-name>
		  <object-name>frntgear-lowercyl</object-name>
              <object-name>lowerstrutf</object-name>
              <object-name>upperstrutf</object-name>
           <property>/gear/gear[0]/caster-angle-deg</property>
             <center>
                    <x-m>-2.741</x-m>
                    <y-m>0</y-m>
                    <z-m>-1.931</z-m>
		</center>
            <axis>
              <x>0</x>
              <y>0</y>
              <z>1</z>
            </axis>
        </animation>


<animation>
            <type>rotate</type>
        <object-name>wheelFR</object-name>
	     <object-name>Cylinder</object-name>
	        <object-name>frntgear-upcyl</object-name>
		  <object-name>frntgear-lowercyl</object-name>
              <object-name>lowerstrutf</object-name>
              <object-name>upperstrutf</object-name>  
           <property>/gear/gear[0]/position-norm</property>
            <interpolation>
                   <entry>
                    <ind>1.0</ind>
                    <dep>0</dep>
                    </entry>
		  <entry>
                    <ind>0</ind>
                    <dep>98</dep>
                    </entry>
	</interpolation>
	 <center>
                    <x-m>-2.771</x-m>
                    <y-m>0</y-m>
                    <z-m>-1.281</z-m>
		</center>
            <axis>
              <x>0</x>
              <y>1</y>
              <z>0</z>
            </axis>
        </animation>

<animation>
            <type>translate</type>
            <object-name>wheelFR</object-name>
	     <object-name>lowerstrutf</object-name>
         <property>/gear/gear[0]/position-norm</property>
            <interpolation>
                    <entry>
                    <ind>1.0</ind>
                    <dep>-0.20</dep>
                    </entry>
		    <entry>
                    <ind>0.5</ind>
                    <dep>-0.13</dep>
                    </entry>
		<entry>
                    <ind>0</ind>
                    <dep>0.0</dep>
                    </entry>
	</interpolation>
	          <center>
                    <x-m>-2.741</x-m>
                    <y-m>0</y-m>
                    <z-m>-1.931</z-m>
		     </center>
            <axis>
              <x>0</x>
              <y>0</y>
              <z>1</z>
            </axis>
        </animation>


<animation>
            <type>translate</type>
            <object-name>wheelFR</object-name>
	     <object-name>lowerstrutf</object-name>
            <property>/gear/gear[0]/compression-norm</property>
            <interpolation>
                    <entry>
                    <ind>0.0</ind>
                    <dep>0.0</dep>
                    </entry>
		<entry>
                    <ind>0.5</ind>
                    <dep>0.114</dep>
                    </entry>
		<entry>
		        <ind>1.0</ind>
                    <dep>0.227</dep>
                    </entry>
	</interpolation>
        <center>
	     <x-m>-2.741</x-m>
           <y-m>0</y-m>
           <z-m>-1.931</z-m>
        </center>
          <axis>
              <x>0</x>
              <y>0</y>
              <z>1</z>
            </axis>
        </animation>
	
<!--variant select animation-->

<animation>
		<type>select</type>
		<!--<object-name>binocular_standart</object-name>-->
		<object-name>longnose</object-name>
		<condition>
			<property>sim/model/aw139/longnose</property>
		</condition>
</animation>

<animation>
		<type>select</type>
		<!--<object-name>binocular_standart</object-name>-->
		<object-name>lshortnose</object-name>
