  </model>

  <!-- ASI -->
  <model>
    <path>Aircraft/aw139/Models/Panel/asi/asi.xml</path>
    <offsets>
      <x-m>  0.213 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  0.435 </z-m>
      <roll-deg>      0.000 </roll-deg>
