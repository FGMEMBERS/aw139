 <!-- Altitude -->
  <model>
    <path>Aircraft/aw139/Models/Panel/alt/alt.xml</path>
    <offsets>
      <x-m>  0.213 </x-m>
      <y-m>  0.175 </y-m>
      <z-m>  0.533 </z-m>
      <roll-deg>    0.000 </roll-deg>
