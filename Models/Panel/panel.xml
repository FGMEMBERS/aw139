<?xml version="1.0"?>

<PropertyList>

 	<path>panel.ac</path>
  <!-- pilot MFD -->
  <model>
    <path>Aircraft/aw139/Models/Panel/fuelvalve/fuel-valve.xml</path>
    <offsets>
      <x-m>  0.224 </x-m>
      <y-m>  -0.201 </y-m>
      <z-m>  0.307 </z-m>
      <roll-deg>    0.000 </roll-deg>
      <pitch-deg>   0.000 </pitch-deg>
      <heading-deg> 0.000 </heading-deg>
    </offsets>
  </model>

   <!-- co-pilot MFD -->
  <model>
    <path>Aircraft/aw139/Models/Panel/fuelvalve/fuel-valve.xml</path>
    <offsets>
      <x-m>  0.224 </x-m>
      <y-m>  -0.201 </y-m>
      <z-m>  0.307 </z-m>
      <roll-deg>    0.000 </roll-deg>
      <pitch-deg>   0.000 </pitch-deg>
      <heading-deg> 0.000 </heading-deg>
    </offsets>
  </model>

