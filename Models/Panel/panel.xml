<animation>
            <type>rotate</type>
            <object-name>wheelR</object-name>
	     <object-name>lowerstrutr</object-name>
	         <object-name>upperstrutr</object-name>
           <property>/gear/gear[2]/position-norm</property>
            <interpolation>
                   <entry>
                    <ind>1.0</ind>
                    <dep>0</dep>
                    </entry>
		  <entry>
                    <ind>0</ind>
                    <dep>90</dep>
                    </entry>
	</interpolation>
	 <center>
                    <x-m>1.76</x-m>
                    <y-m>1.36</y-m>
                    <z-m>-1.124</z-m>
		</center>
            <axis>
              <x>-1</x>
              <y>0</y>
              <z>0</z>
            </axis>
        </animation>
<animation>
            <type>rotate</type>
            <object-name>wheelR</object-name>
	     <object-name>lowerstrutr</object-name>
            <property>/gear/gear[2]/position-norm</property>
            <interpolation>
	          <entry>
                    <ind>0.0</ind>
                    <dep>60</dep>
                    </entry>
                    <entry>
                    <ind>0.7</ind>
                    <dep>45</dep>
                    </entry>
		<entry>
                    <ind>1</ind>
                    <dep>-18.75</dep>
                    </entry>
	</interpolation>
	 <center>
                    <x-m>1.092</x-m>
                    <y-m>1.360</y-m>
                    <z-m>-1.391</z-m>
		</center>
            <axis>
              <x>0</x>
              <y>-1</y>
              <z>0</z>
            </axis>
        </animation>

<animation>
            <type>rotate</type>
            <object-name>wheelR</object-name>
	     <object-name>lowerstrutr</object-name>
            <property>/gear/gear[2]/compression-norm</property>
            <interpolation>
                   <entry>
                    <ind>0.0</ind>
                    <dep>0</dep>
                    </entry>
		 <entry>
                    <ind>0.4</ind>
                    <dep>-15</dep>
                    </entry>
		 <entry>
                    <ind>0.525</ind>
                    <dep>-22</dep>
                    </entry>
		   <entry>
                    <ind>1</ind>
                    <dep>-28</dep>
                    </entry>
	</interpolation>
	 <center>
                    <x-m>1.092</x-m>
                    <y-m>1.360</y-m>
                    <z-m>-1.391</z-m>
		</center>
            <axis>
              <x>0</x>
              <y>1</y>
              <z>0</z>
            </axis>
        </animation>
	


<animation>
            <type>rotate</type>
            <object-name>wheelL</object-name>
	     <object-name>lowerstrutl</object-name>
	     <object-name>upperstrutl</object-name>
           <property>/gear/gear[1]/position-norm</property>
            <interpolation>
                   <entry>
                    <ind>1.0</ind>
                    <dep>0</dep>
                    </entry>
		  <entry>
                    <ind>0</ind>
                    <dep>-90</dep>
                    </entry>
	</interpolation>
	 <center>
                    <x-m>1.76</x-m>
                    <y-m>-1.36</y-m>
                    <z-m>-1.124</z-m>
		</center>
            <axis>
              <x>-1</x>
              <y>0</y>
              <z>0</z>
            </axis>
        </animation>
<animation>
            <type>rotate</type>
            <object-name>wheelL</object-name>
	     <object-name>lowerstrutl</object-name>
        <property>/gear/gear[1]/position-norm</property>
